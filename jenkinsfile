pipeline {
    agent any

    stages {
        stage('parameters') {
            steps {
                script {
                 properties([parameters([gitParameter(branch: '', branchFilter: '.*', defaultValue: 'v1.0', name: 'project-tags', quickFilterEnabled: false, selectedValue: 'NONE', sortMode: 'NONE', tagFilter: '*', type: 'GitParameterDefinition')])])   
                }
            }
        }
    }
}


